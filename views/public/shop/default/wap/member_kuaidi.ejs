<header class="header">
  <a href="javascript:window.history.go(-1);" class="header_ladd1"><span>&nbsp;</span></a>
  <h1 class="head_tit">
    物流查询
  </h1>
  <a href="/wap/"><div class="header_r"><span class="header_cadd1"></span></div></a>
</header>
<%
var company = '商家自有物流';
  if(shipdetail.checkid == 0){
    company = shipdetail.sname;
  }
%>
	<dl class="h_wuliu cf2">
		<dd class="h_wuliur">
			<p>承运公司：<%=company%></p>
			<p>运单编号：<%=goods.shiptypeNo%></p>
      <p>收货人：<%=goods.addrName%></p>
      <p>电话：<%=goods.addrMobile%></p>
      <p>收货地址：<%=goods.addrProvince%><%=goods.addrCity%><%=goods.addrArea%><%=goods.addrAddr%></p>
		</dd>
	</dl>

<div class="h_wulium">
	<p class="h_wuliuma">物流跟踪</p>
  <div class="h_wuliumb">
    <%if(shipdetail.checkid == 0){%>
    <%shipdetail.sdetail.data.Traces.forEach(function(list){%>
      <span><%=list.AcceptStation%><p><%=list.AcceptTime%></p></span>
    <%})%>
    <%}else{%>
      <span>商家自有物流</span>
    <%}%>
  </div>
</div>

