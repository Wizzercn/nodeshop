<div id="typeDialog" class="modal fade bs-modal-sm" tabindex="-2" role="dialog" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">配置商品类型</h4>
      </div>
      <div class="modal-body">
        <div class="row mb10">
          <div class="form-group">
            <label for="is_physical" class="col-sm-4 control-label">是否为实体商品</label>

            <div class="col-sm-6">
              <input type="radio" <%if(obj&&obj.is_physical=='0'){}else{%>checked<%}%> value="1" name="is_physical">是
              <input type="radio" <%if(obj&&obj.is_physical=='0'){%>checked<%}%>value="0" name="is_physical">否
            </div>
          </div>
        </div>
        <div class="row mb10">
          <div class="form-group">
            <label for="use_props" class="col-sm-4 control-label">是否启用商品扩展属性</label>

            <div class="col-sm-6">
              <input type="radio" value="1" name="use_props">是
              <input type="radio" checked value="0" name="use_props">否
            </div>
          </div>
        </div>
        <div class="row mb10">
          <div class="form-group">
            <label for="use_params" class="col-sm-4 control-label">是否启用商品参数表 </label>

            <div class="col-sm-6">
              <input type="radio" value="1" name="use_params">是
              <input type="radio" checked value="0" name="use_params">否
            </div>
          </div>
        </div>
        <div class="row mb10">
          <div class="form-group">
            <label for="use_spec" class="col-sm-4 control-label">是否启用商品规格 </label>

            <div class="col-sm-6">
              <input type="radio" value="1" name="use_spec">是
              <input type="radio" checked value="0" name="use_spec">否
            </div>
          </div>
        </div>
        <div class="row mb10">
          <div class="form-group">
            <label for="use_tabs" class="col-sm-4 control-label">是否启用自定义tab </label>

            <div class="col-sm-6">
              <input type="radio" value="1" name="use_tabs">是
              <input type="radio" checked value="0" name="use_tabs">否
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button id="nextOK" type="button" class="btn btn-primary" data-loading-text="下一步...">下一步</button>
      </div>
    </div>
  </div>
</div>
